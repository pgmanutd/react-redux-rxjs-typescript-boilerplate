{
  "name": "react-redux-rxjs-typescript-boilerplate",
  "version": "1.1.0",
  "description": "React Redux RxJS Typescript Boilerplate",
  "homepage": "https://github.com/pgmanutd/react-redux-rxjs-typescript-boilerplate",
  "author": {
    "name": "pgmanutd",
    "email": "prashantmanunited@gmail.com",
    "url": "https://github.com/pgmanutd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgmanutd/react-redux-rxjs-typescript-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/pgmanutd/react-redux-rxjs-typescript-boilerplate/issues"
  },
  "main": "index.js",
  "engines": {
    "node": ">= 0.6.0"
  },
  "files": [],
  "config": {
    "dirs": {
      "src": "src",
      "dist": "dist",
      "lib": "lib"
    },
    "env": {
      "dev": "development",
      "prod": "production"
    },
    "host": "localhost",
    "port": 3000,
    "productionURL": "",
    "api": {
      "host": "localhost",
      "port": 3002
    },
    "language": "es"
  },
  "scripts": {
    "start": "npm run dev",
    "clean:dist": "rimraf dist",
    "clean:test": "rimraf .nyc_output coverage",
    "lint": "tslint -c tslint.json \"webpack.config*.ts*\" \"typings/**/*.ts*\" \"src/**/*.ts*\" --exclude \"src/**/*.d.ts\" --exclude \"src/**/*.json\"",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run clean:test",
    "test": "nyc npm run test:unit",
    "test:unit": "mocha --opts mocha.opts",
    "test:watch": "mocha --opts mocha.opts --reporter progress --watch",
    "check": "concurrently --prefix \"[{name}]\" --names \"LINT,CLEAN\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run lint\" \"npm run test\"",
    "predev": "npm run clean:dist",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.ts --progress --colors --debug",
    "prebuild": "concurrently --prefix \"[{name}]\" --names \"LINT,CLEAN\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run lint\" \"npm run clean:dist\"",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.ts --progress --colors --profile --bail",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "help": "npm run",
    "release": "./scripts/release.sh"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.d.ts",
      "src/**/*.spec.ts",
      "src/**/*.spec.tsx",
      "src/**/*.json",
      "src/common/testing-utils/*"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": true,
    "report-dir": "./coverage"
  },
  "keywords": [
    "react",
    "redux",
    "rxjs",
    "typescript",
    "boilerplate"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/pgmanutd/react-redux-rxjs-typescript-boilerplate/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "classlist-polyfill": "^1.2.0",
    "classnames": "^2.2.5",
    "clogy": "^1.3.3",
    "es6-promise": "^4.1.0",
    "fastdom": "^1.0.6",
    "lodash": "^4.17.4",
    "offline-plugin": "^4.8.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "rxjs": "^5.4.0"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/chai-enzyme": "^0.6.1",
    "@types/classnames": "2.2.0",
    "@types/compression-webpack-plugin": "^0.3.1",
    "@types/enzyme": "^2.8.0",
    "@types/extract-text-webpack-plugin": "^2.1.0",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jsdom": "^2.0.30",
    "@types/lodash": "^4.14.64",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.22",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.25",
    "@types/react-dom": "^15.5.0",
    "@types/react-helmet": "^5.0.2",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-redux": "^4.4.40",
    "@types/react-router-dom": "^4.0.4",
    "@types/sinon-chai": "^2.7.27",
    "@types/webpack": "^2.2.15",
    "@types/webpack-merge": "0.0.4",
    "awesome-typescript-loader": "^3.1.3",
    "chai": "^4.0.0",
    "chai-enzyme": "^0.7.1",
    "compression-webpack-plugin": "^0.4.0",
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.3",
    "enhanced-resolve": "^3.1.0",
    "enzyme": "^2.8.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "img-loader": "^2.0.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.4.2",
    "nyc": "^10.3.2",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "script-ext-html-webpack-plugin": "^1.8.1",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0",
    "source-map-loader": "^0.2.1",
    "source-map-support": "^0.4.15",
    "style-loader": "^0.18.1",
    "ts-node": "^3.0.4",
    "tsconfig-paths": "^2.2.0",
    "tslint": "^5.3.2",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.3.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  }
}
